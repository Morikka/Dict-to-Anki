function removeWordAndPronounciation(text) {
  let nounIndex;
  let verbIndex;
  let adverbIndex;
  let adjectiveIndex;
  let exclamationIndex;
  if (text.includes("noun")) {
    nounIndex = text.indexOf("noun");
