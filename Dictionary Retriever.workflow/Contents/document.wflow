function formatByPartOfSpeech(text) {
  text = text.replace(" noun ", "\n\nnoun\n");
  text = text.replace(" verb ", "\n\nverb\n");
  text = text.replace(" adverb ", "\n\nadverb\n");
  text = text.replace(" adjective ", "\n\nadjective\n");
